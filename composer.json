            "name": "rahul chavan",
            "email": "rcsoftech85@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.4",
        "symfony/framework-bundle": "^7.4",
        "doctrine/orm": "^3.6",
        "doctrine/doctrine-bundle": "^3.1 || ^2.18",
        "symfony/security-bundle": "^7.4",
        "symfony/http-client": "^7.4",
        "symfony/messenger": "^7.4"
    }
